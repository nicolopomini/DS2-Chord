<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
					<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
						defaultValue="__NULL__"
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="successor_length" displayName="Successor list length" type="int" 
						defaultValue="5" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="nodes" displayName="Number of nodes" type="int" 
						defaultValue="5000" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="join-fail" displayName="Leave/Join per round" type="int" 
						defaultValue="0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="fail_prob" displayName="Failure probability" type="float" 
						defaultValue="0.0" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$FloatConverter"
									
		/>
				<parameter name="keys_exponent" displayName="Exponent (2^M)" type="int" 
						defaultValue="17" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
				<parameter name="type" displayName="Type of run" type="java.lang.String" 
						defaultValue="Standard" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
									values="Standard Disaster "
						
		/>
				<parameter name="rounds" displayName="Number of rounds" type="int" 
						defaultValue="20" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		</parameters>


